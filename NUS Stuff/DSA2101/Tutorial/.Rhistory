drop_na() %>%
distinct()
most_flights <- flights %>%
filter(month == 6, ) %>%
group_by(origin) %>%
count(origin) %>%
ungroup %>%
slice_max(n)
best_ave <- flights %>%
group_by(carrier) %>%
