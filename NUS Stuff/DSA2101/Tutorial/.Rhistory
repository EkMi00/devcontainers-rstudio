library(readxl)
qn1_1 <- read_excel("../data/tourist.xlsx", range="A10:G26")
head(qn1_1)
qn1_2 <- qn1_1[-c(1, 12, 16),-1]
qn1_2 <- apply(qn1_2, 2, function (x) return(as.integer(x)/sum(as.integer(x))))
